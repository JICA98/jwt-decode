{
  "name": "jwt-decode",
  "displayName": "jwt-decode",
  "description": "helps you show the contents of jwt token",
  "repository": {
    "type": "git",
    "url": "https://github.com/JICA98/jwt-decode"
  },
  "publisher": "JICA98",
  "license": "MIT",
  "version": "1.0.1",
  "icon": "icons/icon.png",
  "engines": {
    "vscode": "^1.44.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jwt-decode.decodeJwtToken"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jwt-decode.decodeJwtToken",
        "title": "Decode JWT Token"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.5.5",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "jwt-decode": "^3.1.2"
  }
}